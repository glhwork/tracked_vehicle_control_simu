function PlotPose(pose, figure_x, figure_y)

    f_x = figure_x;
    f_y = figure_y;
    
    p_tmp = zeros(4, size(f_x));
    for i = 1:size(f_x);
        p_tmp(1,1) = f_x(i);
        p_tmp(2,1) = f_y(i);
        p_tmp(4,1) = 1;
    end
    
    for j = 1:size(p_tmp, 2)
        p_tmp(:,j) = pose * p_tmp(:,j);
    end
    
    line1_x = [p_tmp(1,1), p_tmp(1,2)];
    line1_y = []
    plot()

end